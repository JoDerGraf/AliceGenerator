{
    "name": "trappar/alice-generator",
    "description": "Automatically generates alice fixture based on a set of objects",
    "keywords": ["alice", "fixture", "data", "test", "generator", "orm", "doctrine"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Way",
            "email": "jeff.way@me.com"
        }
    ],
    "require": {
        "symfony/yaml": "^3.1",
        "doctrine/common": "^2.6",
        "jms/metadata": "^1.5"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "phpunit/phpunit": "^5.5",
        "nelmio/alice": "^2.2"
    },
    "autoload": {
        "psr-4": { "Trappar\\AliceGenerator\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    }
}
